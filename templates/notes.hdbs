{{#data.notes}}
	<div class="note-comment">
		<div class="content">
			<a class="author">{{added_by}}</a>
	        <div class="metadata">
				<span class="date">{{added_on}}</span>
	        </div>
	        {{#if title}}
		        <div class="title">
		        	{{title}}
		        </div>
	        {{/if}}
	        <div class="text">
	            {{comment}}
	        </div>
	    </div>
	</div>
{{/data.notes}}