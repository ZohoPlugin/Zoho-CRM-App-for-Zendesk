<div class="clear maindiv">
    <div class="pB10"><a class="back_to_contact clrblue">&#171; {{t 'zohocrm_app.back_to_contact'}}</a></div>
    <div>
        <div class="profile_img_div">
            <img src="{{assetURL 'account-logo.png'}}" width="60" height="60" alt="user">
        </div>
        <div class="profile_info">
            <div>
                <div>
                    <div class="zclin15">
                    <a class="open-model bold">{{account.account_name.name}}</a>
                    {{#if account.has_website}}
                      &nbsp;-&nbsp;<span class="zclin15"><a href="{{account.website}}" target="_blank">{{account.website}}</a></span>
                    {{/if}}
                    <a class="openin_newtab" title="{{t 'zohocrm_app.open_in_zohocrm'}}" href="{{account.account_name.link}}" target="_blank"></a>
                    </div>
                    <div class="zclin15">
                        {{#if account.has_industry}}
                          <span class="clr8">{{t 'zohocrm_app.industry'}}:</span>&nbsp;
                          {{account.industry}}
                        {{else}}
                          <span class="clr8">{{t 'zohocrm_app.industry'}}:</span>&nbsp;
                          {{t 'zohocrm_app.not_specified'}}
                        {{/if}}
                    </div>
                    <div class="zclin15">
                        <span class="clr8">{{t 'zohocrm_app.status'}}:</span>&nbsp;
                        {{t 'zohocrm_app.account'}}
                    </div>
                    <div class="zclin15">
                        {{#if account.has_phone}}
                          <span class="clr8">{{t 'zohocrm_app.phone'}}:</span>&nbsp;
                          {{account.phone}}
                        {{else}}
                          <span class="clr8">{{t 'zohocrm_app.phone'}}:</span>&nbsp;
                          {{t 'zohocrm_app.not_specified'}}
                        {{/if}}
                    </div>
                </div>
            </div>
        </div>
        <div class="pT15">
            <ul class="tabs-menu">
                <li class="current crm_fields">{{t 'zohocrm_app.details'}}</li>
                <li class="notes">{{t 'zohocrm_app.notes'}}</li>
                <li class="potentials">{{t 'zohocrm_app.potentials'}}</li>
                <li class="events">{{t 'zohocrm_app.events'}}</li>
            </ul>
        </div>
        <div class="loading_spinner pT10">
  			<h3>{{spinner "dotted"}}</h3>
		</div>
        <div class="data_div" class="clr8 zcctchts pT10">
        </div>
    </div>
</div>
<div id="popupview-model" class="popupview-model modal hide fade" tabindex="-1" role="dialog" aria-labelledby="popupLabel" aria-hidden="true">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
    <h3 id="myModalLabel">{{t 'zohocrm_app.account_information'}}</h3>
  </div>
  <div class="modal-body">
  </div>
  <div class="modal-footer">
    <button class="btn" data-dismiss="modal" aria-hidden="true">{{t 'zohocrm_app.close'}}</button>
  </div>
</div>